{
    "name": "kleister/kleister",
    "version": "1.0.0-alpha1",
    "type": "library",
    "description": "OpenAPI client for Kleister",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api",
        "minecraft"
    ],
    "homepage": "https://github.com/kleister/kleister-php",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Thomas Boerger",
            "email": "thomas@webhippie.de"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^1.7 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "autoload": {
        "psr-4": { "Kleister\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "Kleister\\Test\\" : "test/" }
    }
}
